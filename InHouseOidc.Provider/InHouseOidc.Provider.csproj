<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <PackageId>InHouseOidc.Provider</PackageId>
	<Version>0.0.1</Version>
	<TargetFramework>net6.0</TargetFramework>
	<Authors>Brent Johnson</Authors>
	<Copyright>Brent Johnson 2022</Copyright>
	<Company>InHouseOidc</Company>
	<Description>InHouseOidc provider library.</Description>
	<RepositoryUrl>https://github.com/InHouseOidc/InHouseOidc.Net</RepositoryUrl>
	<RepositoryType>git</RepositoryType>
	<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
	<PackageProjectUrl>https://github.com/InHouseOidc/InHouseOidc.Net</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
	<IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <AdditionalFiles Include="..\stylecop.json" Link="stylecop.json" />
  </ItemGroup>

  <ItemGroup>
	<FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\InHouseOidc.Common\InHouseOidc.Common.csproj" />
  </ItemGroup>

</Project>
